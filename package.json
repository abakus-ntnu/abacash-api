{
  "name": "abacash-api",
  "productName": "AbaCash API",
  "description": "AbaCash API",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abakusbackup/abacash-api.git"
  },
  "author": {
    "name": "Abakus Backup",
    "email": "backup@abakus.no",
    "url": "https://github.com/abakusbackup"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abakusbackup/abacash-api/issues"
  },
  "keywords": [
    "Abakus",
    "Abacash",
    "Sequelize",
    "Express",
    "REST"
  ],
  "homepage": "https://github.com/abakusbackup/abacash-client#readme",
  "scripts": {
    "start": "node dist/index.js",
    "start:watch": "INFLUX=localhost nodemon src/index.js --exec babel-node",
    "build": "npm run clean && npm run dist",
    "dist": "babel src --out-dir dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "load-db": "PG_URL=${PG_URL:-'postgres://abacash:@localhost/abacash'} babel-node bin/load-db.js",
    "setup-stats": "INFLUX=localhost  babel-node bin/setup-stats.js",
    "migrate": "PG_URL=${PG_URL:-'postgres://abacash:@localhost/abacash'}  babel-node bin/migrate.js",
    "test": "PG_URL=${PG_URL:-'postgres://abacash:@localhost/abacash'} INFLUX=localhost NODE_ENV=test nyc --require babel-register --require babel-polyfill mocha $(find test -name '*.test.js')",
    "test:watch": "npm test -- --watch --reporter min",
    "lint": "eslint --ignore-path .gitignore .",
    "clean": "rm -rf /dist"
  },
  "dependencies": {
    "bcrypt": "1.0.2",
    "bluebird": "3.4.7",
    "body-parser": "1.16.0",
    "continuation-local-storage": "3.2.0",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "coveralls": "2.11.15",
    "errorhandler": "1.5.0",
    "express": "4.14.1",
    "express-handlebars": "3.0.0",
    "influx": "5.0.0-alpha.4",
    "jsonfile": "^2.4.0",
    "jsonwebtoken": "7.2.1",
    "lodash": "4.17.2",
    "method-override": "2.3.7",
    "morgan": "1.8.0",
    "pg-promise": "^5.5.6",
    "nodemailer": "3.0.2",
    "nodemailer-express-handlebars": "^2.0.0",
    "pg": "^6.1.0",
    "raven": "0.12.1",
    "superagent": "3.4.1",
    "sequelize": "3.30.1",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-core": "6.7.4",
    "babel-eslint": "7.1.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "eslint": "3.10.2",
    "eslint-config-airbnb": "9.0.0",
    "eslint-config-webkom": "1.4.1",
    "eslint-plugin-import": "2.2.0",
    "mocha": "2.4.5",
    "nyc": "10.1.2",
    "sequelize-fixtures": "0.5.5",
    "supertest": "3.0.0"
  },
  "private": true
}
