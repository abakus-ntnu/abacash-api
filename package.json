{
  "name": "abacash-api",
  "productName": "AbaCash API",
  "description": "AbaCash API",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abakusbackup/abacash-api.git"
  },
  "author": {
    "name": "Abakus Backup",
    "email": "backup@abakus.no",
    "url": "https://github.com/abakusbackup"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abakusbackup/abacash-api/issues"
  },
  "keywords": ["Abakus", "Abacash", "Sequelize", "Express", "REST"],
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "babel": {
    "presets": ["es2015", "stage-0"]
  },
  "homepage": "https://github.com/abakusbackup/abacash-client#readme",
  "scripts": {
    "start": "node dist/index.js",
    "start:watch": "nodemon src/index.js --exec babel-node",
    "build": "npm run clean && npm run dist",
    "dist": "babel src --out-dir dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "load-db": "babel-node bin/load-db.js",
    "test":
      "NODE_ENV=test nyc --require babel-register --require babel-polyfill mocha --exit $(find test -name '*.test.js')",
    "test:watch": "npm test -- --watch --reporter min",
    "lint": "yarn run lint:js && yarn run lint:prettier",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:prettier": "prettier '**/*.{js,css,md}' --list-different",
    "prettier": "prettier '**/*.{js,css,md}' --write",
    "clean": "rm -rf /dist"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cls-bluebird": "^2.1.0",
    "continuation-local-storage": "^3.2.1",
    "cookie-parser": "1.4.3",
    "cors": "^2.8.4",
    "coveralls": "^3.0.0",
    "errorhandler": "1.5.0",
    "express": "^4.16.2",
    "express-handlebars": "3.0.0",
    "forest-express-sequelize": "^2.4.2",
    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.17.5",
    "method-override": "^2.3.10",
    "morgan": "^1.9.0",
    "nodemailer": "^4.4.2",
    "nodemailer-express-handlebars": "^3.0.0",
    "pg": "^7.4.1",
    "pino": "^4.11.0",
    "raven": "^2.4.1",
    "sequelize": "^4.33.4",
    "superagent": "^3.8.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.18.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.6.0",
    "mocha": "^5.0.1",
    "nodemon": "^1.17.3",
    "nyc": "^11.4.1",
    "prettier": "^1.10.2",
    "sequelize-fixtures": "^0.7.0",
    "supertest": "3.0.0"
  },
  "private": true
}
