{
  "name": "abacash",
  "version": "0.0.0",
  "scripts": {
    "watch": "nodemon --exec npm start",
    "start": "babel-node src/index.js",
    "load-db": "PG_URL=${PG_URL:-'postgres://localhost/abacash'} babel-node bin/load-db.js",
    "test": "PG_URL=${PG_URL:-'postgres://localhost/abacash-test'} NODE_ENV=test mocha --compilers js:babel-core/register $(find test -name '*.test.js')",
    "test:watch": "npm test -- --watch --reporter min",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "dependencies": {
    "babel-cli": "6.4.5",
    "babel-core": "6.4.5",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-0": "6.3.13" ,
    "bcrypt": "0.8.5",
    "bluebird": "3.2.1",
    "body-parser": "1.5.0",
    "compression": "1.6.1",
    "continuation-local-storage": "3.1.6",
    "cookie-parser": "1.0.1",
    "cors": "2.7.1",
    "errorhandler": "1.4.3",
    "express": "4.13.4",
    "express-jwt": "3.3.0",
    "jsonwebtoken": "5.5.4",
    "lodash": "4.2.0",
    "method-override": "1.0.0",
    "morgan": "1.6.1",
    "pg": "4.4.4",
    "pg-hstore": "2.3.2",
    "sequelize": "3.19.0",
    "superagent": "1.7.2"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "chai": "^3.4.0",
    "chai-as-promised": "^5.1.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-config-webkom": "^1.2.0",
    "mocha": "^2.3.3",
    "sequelize-fixtures": "git+https://github.com/abakusbackup/sequelize-fixtures.git",
    "supertest": "^1.1.0"
  },
  "private": true
}
