{
  "name": "abacash-api",
  "productName": "AbaCash API",
  "description": "AbaCash API",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abakusbackup/abacash-api.git"
  },
  "author": {
    "name": "Abakus Backup",
    "email": "backup@abakus.no",
    "url": "https://github.com/abakusbackup"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abakusbackup/abacash-api/issues"
  },
  "keywords": [
    "Abakus",
    "Abaash"
  ],
  "homepage": "https://github.com/abakusbackup/abacash-client#readme",
  "scripts": {
    "start": "node dist/index.js",
    "start:watch": "nodemon src/index.js --exec babel-node",
    "build": "npm run clean && npm run dist",
    "dist": "babel src --out-dir dist",
    "load-db": "PG_URL=${PG_URL:-'postgres://localhost/abacash'} babel-node bin/load-db.js",
    "test": "PG_URL=${PG_URL:-'postgres://localhost/abacash-test'} NODE_ENV=test mocha --compilers js:babel-core/register $(find test -name '*.test.js')",
    "test:watch": "npm test -- --watch --reporter min",
    "lint": "eslint --ignore-path .gitignore .",
    "clean": "rm -rf /dist",
    "transfer": "PG_URL=${PG_URL:-'postgres://localhost/abacash'} node scripts/transfer.js"
  },
  "dependencies": {
    "bcrypt": "0.8.6",
    "bluebird": "3.4.0",
    "body-parser": "1.15.1",
    "compression": "1.6.2",
    "continuation-local-storage": "3.2.0",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "errorhandler": "1.4.3",
    "express": "4.13.4",
    "express-handlebars": "^3.0.0",
    "express-jwt": "3.4.0",
    "jsonwebtoken": "7.0.0",
    "lodash": "4.16.6",
    "method-override": "2.3.6",
    "morgan": "1.7.0",
    "nodemailer": "2.4.2",
    "nodemailer-express-handlebars": "^2.0.0",
    "pg": "6.0.0",
    "pg-hstore": "2.3.2",
    "raven": "0.12.1",
    "sequelize": "3.23.3",
    "superagent": "2.3.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "commander": "^2.9.0",
    "eslint": "2.11.1",
    "eslint-config-airbnb": "^9.0.0",
    "eslint-config-webkom": "^1.3.4",
    "eslint-plugin-import": "^1.6.1",
    "mocha": "^2.4.5",
    "mongodb": "^2.1.16",
    "nodemailer-stub-transport": "^1.0.0",
    "sequelize-fixtures": "git+https://github.com/abakusbackup/sequelize-fixtures.git",
    "sinon": "^1.17.4",
    "supertest": "^1.2.0"
  },
  "private": true
}
