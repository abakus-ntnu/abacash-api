    "test": "PG_URL=${PG_URL:-'postgres://localhost/abacash-test'} NODE_ENV=test mocha --compilers js:babel/register $(find test -name '*.test.js')",
    "test-watch": "npm test -- --watch --reporter min",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "dependencies": {
    "babel": "~5.8.23",
    "bcrypt": "^0.8.5",
    "bluebird": "~3.0.5",
    "body-parser": "~1.5.0",
    "compression": "~1.0.1",
    "continuation-local-storage": "^3.1.4",
    "cookie-parser": "~1.0.1",
    "cors": "^2.7.1",
    "errorhandler": "~1.0.0",
    "express": "~4.0.0",
    "jsonwebtoken": "^5.5.4",
    "lodash": "~2.4.1",
    "method-override": "~1.0.0",
    "morgan": "~1.0.0",
    "passport": "0.3.2",
    "passport-jwt": "^1.2.1",
    "pg": "4.4.2",
    "pg-hstore": "2.3.2",
    "sequelize": "3.12.0"
  },
  "devDependencies": {
    "babel-eslint": "~4.1.3",
    "chai": "~3.4.0",
    "chai-as-promised": "~5.1.0",
    "eslint": "~1.6.0",
    "eslint-config-airbnb": "~0.1.0",
    "eslint-config-webkom": "~0.3.1",
    "mocha": "~2.3.3",
